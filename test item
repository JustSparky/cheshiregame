#include <iostream>

using namespace std;

struct item
{
    string name;  
    int attack;   //if weapon
    int defense;  //if armor
    int value;    //store value
};


int main()
{
    //example item
    item sword;
    sword.attack = 5;
    sword.defense = 0;
    sword.value = 10;
    

    return 0;
}
